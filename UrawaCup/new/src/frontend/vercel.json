{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://urawacup-backend.onrender.com/api",
      "VITE_WS_URL": "wss://urawacup-backend.onrender.com/ws"
    }
  }
}
